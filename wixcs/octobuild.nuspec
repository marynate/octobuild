<?xml version="1.0"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata>
    <id>octobuild-$target$</id>
    <title>octobuild-$target$</title>
    <version>$version$</version>
    <authors>Artem V. Navrotskiy</authors>
    <description>Simple distributed compile system for C++ ($target$ version)</description>
    <projectUrl>https://github.com/bozaro/octobuild</projectUrl>
    <licenseUrl>https://raw.githubusercontent.com/bozaro/octobuild/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>
    <file src="release\xgconsole.exe" />
    <file src="release\*.dll" />
  </files>
</package>
